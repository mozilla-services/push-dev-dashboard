extends: eslint:recommended

env:
    browser: true,
    jquery: true,
    node: true

globals:
    dashboard: false
    ga: false

rules:
    curly: 0
    eqeqeq: 2
    guard-for-in: 2
    indent: [2, 4, {SwitchCase: 1}]
    no-bitwise: 2
    no-caller: 2
    no-empty: 2
    no-extend-native: 2
    no-irregular-whitespace: 2
    no-undef: 2
    no-unused-vars: 2
    quotes: [2, single]
    strict: [2, function]
